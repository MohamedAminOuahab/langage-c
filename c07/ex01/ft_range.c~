#include <stdlib.h>
#include <stdio.h>
int	ft_atoi(char	*str)
{
	int i;
	int res;
	int sign;

	i = 0;
	res = 0;
	sign = 1;
	while((str[i] < 14 && str[i] > 8) || str[i] == 32)
		i++;
	if(str[i] == '-' || str[i] == '+')
	{
		if(str[i] == '-')
			sign *= -1;
		i++;
	}
	while(str[i] >= '0' && str[i] <= '9')
	{
		res = res * 10 + str[i] - '0';
		i++;
	}
	return (res * sign);
}

int	*ft_range(int	min, int	max)
{
	int	*vlr;
	int	i;

	if (min >= max)
	       return (NULL);	
	vlr = (int*) malloc(sizeof(int) * (max - min));
	i = 0;
	while(i < max - min)
	{
		vlr[i] = min + i;
		i++;
	}	
	return (vlr);
}

int	main(int	ac, char	**av)
{
	printf("%d",ft_range(1,2));
	return 0;
}
